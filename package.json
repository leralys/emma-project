{
  "name": "@emma-project/source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "pnpm run dev:all",
    "dev:all": "nx run-many -t serve --projects=frontend,backend --parallel=2",
    "dev:frontend": "nx serve frontend",
    "dev:backend": "nx serve backend",
    "build": "pnpm run build:all",
    "build:all": "nx run-many -t build --projects=frontend,backend --parallel=2",
    "build:frontend": "nx build frontend",
    "build:backend": "nx build backend",
    "test": "nx run-many -t test --all",
    "test:frontend": "nx test frontend",
    "test:backend": "nx test backend",
    "e2e": "nx run-many -t e2e --all",
    "e2e:frontend": "nx e2e frontend-e2e",
    "e2e:backend": "nx e2e backend-e2e",
    "lint": "nx run-many -t lint --all",
    "lint:frontend": "nx lint frontend",
    "lint:backend": "nx lint backend",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "nx run-many -t typecheck --all",
    "clean": "nx reset && rm -rf dist node_modules",
    "graph": "nx graph",
    "prepare": "husky",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "generate:vapid": "tsx scripts/generate-vapid-keys.ts"
  },
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.916.0",
    "@aws-sdk/s3-request-presigner": "^3.916.0",
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@prisma/adapter-neon": "^6.18.0",
    "@prisma/client": "^6.18.0",
    "argon2": "^0.44.0",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "create-nx": "0.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sonner": "^2.0.7",
    "web-push": "^3.6.7",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,prisma}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/devkit": "22.0.1",
    "@nx/eslint": "22.0.1",
    "@nx/eslint-plugin": "22.0.1",
    "@nx/js": "22.0.1",
    "@nx/nest": "^22.0.1",
    "@nx/node": "22.0.1",
    "@nx/playwright": "22.0.1",
    "@nx/react": "^22.0.1",
    "@nx/vite": "22.0.1",
    "@nx/web": "22.0.1",
    "@nx/webpack": "22.0.1",
    "@nx/workspace": "22.0.1",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/web-push": "^3.6.4",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^16.2.6",
    "nx": "22.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.16",
    "tslib": "^2.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.0",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^5.1.4",
    "workbox-window": "^7.3.0"
  }
}
